# 仅对 Node 环境有效，默认为控制台输出
# 日志级别: trace | debug | info | warn | error | fatal
LOG_LEVEL=info
# 是否开启 pretty 输出 (true/false)
LOG_PRETTY=true
# AES-256-GCM 加密密钥 (64 位十六进制 / 32 字节)
ENCRYPTION_KEY=
# JWT 文件密钥签名用的共享密钥
FILE_KEY_SECRET=
# AI 相关配置
OPENAI_API_KEY=
OPENAI_API_URL=
OPENAI_MODEL=
# Resumable stream (optional)
REDIS_URL=
# 数据库
DATABASE_URL=postgresql://user:password@localhost:5432/mydb
# Better Auth (@my-monorepo/auth)
BETTER_AUTH_URL=http://localhost:3000
TAURI_URL=http://localhost:1420
# API server base URL
VITE_API_URL=http://localhost:5173
BETTER_AUTH_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# 邮件服务 (@my-monorepo/utils/email)
MAIL_HOST=smtp.example.com
MAIL_PORT=587
MAIL_SECURE=false
MAIL_AUTH_USER=
MAIL_AUTH_PASS=
MAIL_FROM="My App <noreply@example.com>"
